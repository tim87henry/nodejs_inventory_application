<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <h1>List of Genres</h1>
    <% genres.forEach(genre => { %>
      <li><a href=<%= genre.url %>><%= genre.name %></a></li>
    <% }) %>
  </body>
</html>
